### Custom Aliases ###

alias ll='ls -lah'
alias h='~'
alias netb='/Applications/netbeans/bin/./netbeans'
alias vgs='vagrant global-status'
alias buu='brew update && brew upgrade && brew cask upgrade'
alias au='autopkg'
alias dkl='diskutil list'
alias p3='ping -c 3'


### Functions ###

   	lazyg ()
		{
		git add .
		git commit -a -m "$1"
		git push -u origin master
		}

	mkd () {
		mkdir "$1"
		cd "$1"
			}






